    //
    function endtime(){
      var EndTime = new Date('2018/03/02 00:00:00');
      var NowTime = new Date();
      var t = EndTime.getTime() - NowTime.getTime();
      var d = Math.ceil(t / 1000 / 60 / 60 / 24);
      var nowDayShow = '';
      if (d == 2) {
        nowDayShow = '后天元宵节，';
      } else if (d == 1) {
        nowDayShow = '明天元宵节，';
      } else if (d <= 0) {
        nowDayShow = '元宵节到了，';
      } else {
        nowDayShow =  d + '天后元宵节，';
      }
      return nowDayShow;
    }
